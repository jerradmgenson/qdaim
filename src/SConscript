Import('env')
Import('GIT_ROOT')
Import('INGESTED_DIR')
DATA_DIR = GIT_ROOT / 'data'

cleveland = env.Ingest_cleveland_data(str(INGESTED_DIR / 'cleveland'),
                                      str(DATA_DIR / 'cleveland'))

preprocessed = env.Preprocess(cleveland)
qdaim = env.Gen_model('qdaim', 'gen_model_config')
Depends(qdaim, preprocessed)
Clean(qdaim, 'gen_model_config.json')
NoClean(qdaim)
